{
  "name": "my-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.4",
    "cors": "^2.8.5",
    "cra-template": "1.2.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-jest": "^27.2.1",
    "firefox-profile": "^4.3.2",
    "http-proxy-middleware": "^2.0.6",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.3",
    "react-bootstrap-icons": "^1.10.3",
    "react-calendar": "^4.2.1",
    "react-click-away-listener": "^2.2.3",
    "react-cookie": "^4.1.1",
    "react-date-picker": "^10.0.3",
    "react-datetime-picker": "^5.0.3",
    "react-dom": "^18.2.0",
    "react-floating-button": "^1.1.1",
    "react-idle-timer": "^5.5.3",
    "react-jwt": "^1.1.8",
    "react-moveable": "^0.48.0",
    "react-ratings-declarative": "^3.4.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.2",
    "react-time-picker": "^6.0.4",
    "react-toastify": "^9.1.2",
    "serve": "^14.2.0"
  },
  "scripts": {
    "start": "cross-env HOST=localhost NODE_ENV=production node_modules/react-scripts/bin/react-scripts.js start",
    "prod_start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll --ci --no-watchAll --passWithNoTests",
    "test:coverage": "npm test -- --watchAll --ci --no-watchAll --coverage --collectCoverageFrom='./src/**'",
    "test:watch": "npm test -- --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix --ext .js,.jsx ."
  },
  "eslintConfig": {
    "rules": {
        "jest/no-disabled-tests": "warn",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error",
        "jest/prefer-to-have-length": "warn",
        "jest/valid-expect": "error"
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
        "jest/globals": true
    }
  },
  "prettier": "@imaginary-cloud/prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "prettier": "^2.8.7",
    "react-router-dom": "^6.10.0",
    "web-vitals": "^3.3.1"
  }
}
